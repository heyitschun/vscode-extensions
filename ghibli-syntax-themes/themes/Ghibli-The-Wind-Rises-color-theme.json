    "tokenColors": [
        {
            "name": "Comments",
            "scope": "comment",
            "settings": {
                "foreground": "#b4986b",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Constants",
            "scope": "constant",
            "settings": {
                "foreground": "#0096c3"
            }
        },
        {
            "name": "Escape characters",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#16a13a"
            }
        },
        {
            "name": "Named entities",
            "scope": "entity.name",
            "settings": {
                "foreground": "#c6398b"
            }
        },
        {
            "name": "Function names",
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#16a13a"
            }
        },
        {
            "name": "Tags",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#ab524f",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Static types",
            "scope": [
                "entity.name.type",
                "storage.type.cs"
            ],
            "settings": {
                "foreground": "#c6398b"
            }
        },
        {
            "name": "Tag attributes",
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#0096c3",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Inherited classes",
            "scope": "entity.other.inherited-class",
            "settings": {
                "foreground": "#d87e43"
            }
        },
        {
            "name": "CSS ID selectors",
            "scope": "entity.other.attribute-name.id",
            "settings": {
                "foreground": "#16a13a"
            }
        },
        {
            "name": "Pseudo CSS",
            "scope": [
                "entity.other.attribute-name.pseudo-element",
                "entity.other.attribute-name.pseudo-class"
            ],
            "settings": {
                "foreground": "#d87e43"
            }
        },
        {
            "name": "Variable names",
            "scope": [
                "entity.name.variable",
                "variable"
            ],
            "settings": {
                "foreground": "#3b2026"
            }
        },
        {
            "name": "Keywords",
            "scope": "keyword",
            "settings": {
                "foreground": "#3e98a3",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#ab8a90"
            }
        },
        {
            "name": "Special operators",
            "scope": [
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.logical",
                "keyword.operator.delete"
            ],
            "settings": {
                "foreground": "#3e98a3"
            }
        },
        {
            "name": "Units",
            "scope": "keyword.other.unit",
            "settings": {
                "foreground": "#0096c3"
            }
        },
        {
            "name": "Markup quotes",
            "scope": "markup.quote",
            "settings": {
                "foreground": "#FAB795B3",
            }
        },
        {
            "name": "Markup headings",
            "scope": [
                "markup.heading",
                "entity.name.section"
            ],
            "settings": {
                "foreground": "#3b2026"
            }
        },
        {
            "name": "Markup bold",
            "scope": "markup.bold",
            "settings": {
                "foreground": "#3e98a3",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup italics",
            "scope": "markup.italic",
            "settings": {
                "foreground": "#16a13a",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markup code",
            "scope": [
                "markup.inline.raw",
                "markup.fenced_code.block"
            ],
            "settings": {
                "foreground": "#0096c3"
            }
        },
        {
            "name": "Markup links",
            "scope": "markup.underline.link",
            "settings": {
                "foreground": "#d87e43"
            }
        },
        {
            "name": "Storage keywords",
            "scope": "storage",
            "settings": {
                "foreground": "#3e98a3",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string.quoted",
                "string.template"
            ],
            "settings": {
                "foreground": "#d87e43"
            }
        },
        {
            "name": "Regex",
            "scope": "string.regexp",
            "settings": {
                "foreground": "#0096c3"
            }
        },
        {
            "name": "Markup link descriptions",
            "scope": "string.other.link",
            "settings": {
                "foreground": "#0096c3"
            }
        },
        {
            "name": "Provided (support) values",
            "scope": "support",
            "settings": {
                "foreground": "#c6398b"
            }
        },
        {
            "name": "Provided functions",
            "scope": "support.function",
            "settings": {
                "foreground": "#16a13a"
            }
        },
        {
            "name": "Provieded variables",
            "scope": "support.variable",
            "settings": {
                "foreground": "#3b2026"
            }
        },
        {
            "name": "Object/JSON property names",
            "scope": [
                "support.type.property-name",
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#0096c3"
            }
        },
        {
            "name": "CSS property names",
            "scope": "support.type.property-name.css",
            "settings": {
                "foreground": "#ab8a90"
            }
        },
        {
            "name": "Language variables",
            "scope": [
                "variable.language"
            ],
            "settings": {
                "foreground": "#c6398b",
            }
        },
        {
            "name": "Embedded template punctuation",
            "scope": "string.template meta.embedded",
            "settings": {
                "foreground": "#ab8a90"
            }
        },
        {
            "name": "Tag brackets",
            "scope": "punctuation.definition.tag",
            "settings": {
                "foreground": "#b17570",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Key/value separators",
            "scope": "punctuation.separator",
            "settings": {
                "foreground": "#ab8a90"
            }
        },
        {
            "name": "Template expressions",
            "scope": "punctuation.definition.template-expression",
            "settings": {
                "foreground": "#3e98a3"
            }
        },
        {
            "name": "Embedded section punctuation",
            "scope": "punctuation.section.embedded",
            "settings": {
                "foreground": "#3e98a3"
            }
        },
        {
            "name": "Markup list punctuation",
            "scope": "punctuation.definition.list",
            "settings": {
                "foreground": "#0096c3"
            }
        }
    ]
}